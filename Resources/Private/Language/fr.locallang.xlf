            </trans-unit>
            <trans-unit id="dismiss">
                <source>allow cookies</source>
                <target>Accepter les cookies !</target>
            </trans-unit>
            <trans-unit id="allow">
                <source>save</source>
                <target>Sauvegarder</target>
            </trans-unit>
            <trans-unit id="allowall">
                <source>allow all!</source>
                <target>Accepter tous !</target>
            </trans-unit>
            <trans-unit id="config">
                <source>customize</source>
                <target>personnaliser</target>
            </trans-unit>
            <trans-unit id="deny">
                <source>decline</source>
                <target>Refuser</target>
            </trans-unit>
            <trans-unit id="policy">
                <source>Cookie Policy</source>
                <target>Cookies</target>
            </trans-unit>
            <trans-unit id="link">
                <source>More info</source>
                <target>Plus d'infos</target>
            </trans-unit>
            <trans-unit id="dpRequire">
                <source>necessary</source>
                <target>Nécessaire</target>
            </trans-unit>
            <trans-unit id="dpStatistik">
                <source>statistics</source>
                <target>Statistiques</target>
            </trans-unit>
            <trans-unit id="dpMarketing">
                <source>marketing</source>
                <target>Marketing</target>
            </trans-unit>

